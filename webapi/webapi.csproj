    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="az-900.quiz" />
    <None Remove="PSM 1.quiz" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="az-900.quiz">
